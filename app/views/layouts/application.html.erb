  <title>SimpleApp</title>
   <!-- <meta name="viewport" content="width=device-width" /> -->
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <!-- <link href="bootstrap-responsive.css" rel="stylesheet"> -->
  
  <%= stylesheet_link_tag    'bootstrap-responsive', media: 'all' %>
  <%= stylesheet_link_tag    'case_histories', media: 'all' %>
  <%= csrf_meta_tags %>
  
</head>

<body style="padding-left: 0; padding-right: 0; ">

  <header class="menu">
    <nav class="navbar navbar-default" role="navigation" style="border-radius: 0px;">
      <div class="container-fluid">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a href="http://enbage.it/" class="navbar-brand" style="margin-top: 13px; display: inline-block; margin-bottom: 13px;border-bottom: 1px solid #cab340;">
            <%= image_tag "enbage_logo.png" %>
          </a>
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse ulNav" id="bs-example-navbar-collapse-1">
          <ul id="navcontainer"style="">
            <% if @case_history && !@case_history.new_record?  %>          
              <li><a href="#mission"class="anchor"class="active">Mission</a></li>
              <li><a href="#solution"class="anchor"class="active">Solution</a></li>
              <li><a href="#result"class="anchor"class="active">Result</a></li> 
              <li style="font-family: roboto;font-size: 20px;font-weight: bold;">|</li> 
              <li><%= link_to "Edit", edit_case_history_path(@case_history) %></li>
              <li><a href="/case_histories">Back</a></li>  
              <% else %>
                <% if current_page?(case_histories_path) %>
                  <li><%= link_to 'New Case history', new_case_history_path %></li>
                  <% else %>
                    <li><a href="/case_histories">Back</a></li>
                <% end %>
            <% end %>
          </ul>
        </div><!-- /.navbar-collapse -->
      </div><!-- /.container-fluid -->
    </nav>
  </header>
